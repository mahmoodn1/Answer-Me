<h1>Admin : Listing Items</h1>
<%= link_to 'New Post', new_admin_item_path, class: 'btn btn-link' %>

<ul class="list-group">
  <%= render @admin_items %>
</ul>


<% content_for :javascripts do %>
  <%= javascript_include_tag 'admin/root' %>
  <script>
    window.current_user = <%= current_user.to_json.html_safe %>
  </script>
<% end %>

