<!DOCTYPE html>
<html>
<head>
  <title><%= "#{yield(:title)} " %><%= site_title_text %></title>
  <link rel="shortcut icon" href="https://engineering.purdue.edu/Wraps/ECO/wrap7/themes/default/common/favicon.ico">
  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <%= include_sync_config %>
</head>
<body style="padding-top:70px">
<%= render 'components/top_navigation' %>
<%= yield :headerwrap %>

<div id="service">
  <div class="container">
    <%= yield %>
  </div>
</div>

<%= render 'components/footer' %>

<%= render 'components/google_analytics' %>
<%= yield :javascripts %>
</body>

</html>
